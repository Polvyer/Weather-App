{
    "ignorePatterns": ["*bundle.js"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "import/prefer-default-export": ["off", "always"],
      "no-console": ["off", "always"],
      "no-use-before-define": ["off", "always"],
      "no-else-return": ["off", "always"]
    }
}
